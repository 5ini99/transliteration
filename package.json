{
  "name": "transliteration",
  "version": "1.1.0",
  "description": "Unicode to ACSII transliteration / slugify module. Works in node.js, web browser and command line.",
  "main": "build/node/index.js",
  "scripts": {
    "lint": "eslint lib/**/*.js test/**/*.js gulpfile.babel.js",
    "pretest": "npm run lint",
    "test": "npm run test:node",
    "test:node": "tape -r babel-core/register test/node/*.js | tap-spec",
    "test:browser": "zuul --no-coverage -- test/browser/*.js",
    "prebuild": "npm run test",
    "build": "gulp",
    "coverage": "nyc npm test",
    "postcoverage": "rimraf .nyc_output",
    "coveralls": "nyc npm test && nyc report -r=text-lcov | coveralls",
    "postcoveralls": "rimraf .nyc_output"
  },
  "bin": {
    "transliterate": "build/bin/transliterate",
    "slugify": "build/bin/slugify"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/andyhu/node-transliteration.git"
  },
  "keywords": [
    "transliterate",
    "transliteration",
    "utf8",
    "slug",
    "slugify",
    "romanization",
    "unicode",
    "unidecode"
  ],
  "author": "Andy Hu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andyhu/node-transliteration/issues"
  },
  "devDependencies": {
    "babel-core": "^6.9.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015-loose": "^7.0.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "coveralls": "^2.11.9",
    "es5-shim": "^4.5.8",
    "eslint": "^2.11.1",
    "eslint-config-airbnb-base": "^3.0.1",
    "eslint-plugin-import": "^1.8.1",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-es3ify": "0.0.0",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^1.5.3",
    "gulp-util": "^3.0.7",
    "nyc": "^6.4.4",
    "rimraf": "^2.5.2",
    "tap-spec": "^4.1.1",
    "tape": "^4.5.1",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "zuul": "^3.10.1"
  },
  "dependencies": {
    "yargs": "^4.7.1"
  }
}
